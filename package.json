{
    "name": "owlcode",
    "displayName": "owlcode",
    "description": "",
    "version": "0.0.1",
    "publisher": "owlcode",
    "engines": {
        "vscode": "^1.28.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.headerLV1",
        "onCommand:extension.headerLV2",
        "onCommand:extension.dropbox.transform_raw_uri"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.headerLV1",
                "title": "Add header LV1"
            },
            {
                "command": "extension.headerLV2",
                "title": "Add header LV2"
            },
            {
                "command": "extension.dropbox.transform_raw_uri",
                "title": "Transform to raw URI in dropbox"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.21",
        "tslint": "^5.8.0",
        "@types/node": "^8.10.25",
        "@types/mocha": "^2.2.42"
    }
}
